{
  "name": "bachelor-fantasy-api",
  "version": "0.0.1",
  "main": "server.ts",
  "repository": "https://github.com/bbush915/bachelor-fantasy-api.git",
  "author": "Bryan Bush <bryan@dialexa.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "start": "node ./dist/main.js",
    "start:dev": "ts-node-dev --require tsconfig-paths/register src/server.ts",
    "migrate:make": "ts-node-dev node_modules/.bin/knex migrate:make --knexfile db/knexfile.ts",
    "migrate:up": "ts-node-dev node_modules/.bin/knex migrate:latest --knexfile db/knexfile.ts",
    "migrate:down": "ts-node-dev node_modules/.bin/knex migrate:rollback --knexfile db/knexfile.ts",
    "seed:run": "ts-node-dev node_modules/.bin/knex seed:run --knexfile db/knexfile.ts",
    "tunnel:open": "bash scripts/tunnel-open.sh",
    "tunnel:close": "bash scripts/tunnel-close.sh",
    "scores:export": "ts-node-dev scripts/scores-export.ts",
    "scores:import": "ts-node-dev scripts/scores-import.ts",
    "week:advance": "ts-node-dev scripts/advance-current-week.ts"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "apollo-server-koa": "^2.21.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.2",
    "knex-stringcase": "^1.4.5",
    "koa": "^2.13.1",
    "koa-cors": "^0.0.16",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa": "^2.13.1",
    "@types/koa-cors": "^0.0.0",
    "@types/minimist": "^1.2.1",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/webpack": "^4.41.26",
    "@types/webpack-node-externals": "^2.5.0",
    "dotenv": "^8.2.0",
    "minimist": "^1.2.5",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.18",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
